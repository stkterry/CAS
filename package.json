{
  "name": "@stkterry/cah",
  "version": "0.1.0",
  "description": "A CAH clone",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "server:debug": "nodemon --inspect app.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "sass": "sass --watch client/src/assets/stylesheets/input.scss:client/src/assets/stylesheets/App.css",
    "dev": "concurrently \"npm run server:debug\" \"npm run client\" \"npm run sass\"",
    "dropDB": "node -e 'require(\"./server/seed/seeder.js\").dropDB()'",
    "dropDBC": "node -e 'require(\"./server/seed/seeder.js\").dropDBC()'",
    "seedDB": "node -e 'require(\"./server/seed/seeder.js\").seedDB()'",
    "seedCards": "node -e 'require(\"./server/seed/cardSeed.js\").seedCards()'"
  },
  "author": "Steven K Terry",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mongoose": "^5.9.22",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-fittext": "^1.0.0",
    "react-textfit": "^1.1.0",
    "simplebar-react": "^2.2.1",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "chance": "^1.1.6",
    "concurrently": "^5.2.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4"
  }
}
